function [ coalitions ] = Merge( partition )

coalitions={};
for i=1:size(partition,1)-1
    for j=1:size(partition,2)-1
      for k=2:size(partition,1)
          for m=2:size(partition,2)
            newCoalition=[partition(i)(j) partition(j)];
            coalitions(i,j)= {newCoalition};
          end
      end
    end
end

end

